# Environment Configuration Example
# Copy this file to .env.development, .env.staging, or .env.production
# Update the values according to your environment

# API Configuration
API_BASE_URL=https://api.mlmrealestate.com
API_KEY=your_api_key_here
API_TIMEOUT=30000

# Environment
ENVIRONMENT=production
DEBUG_MODE=false

# Firebase (optional - usually configured in Firebase config files)
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_APP_ID=your-app-id

# Google Maps
MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY

# Payment Gateway
RAZORPAY_KEY=rzp_live_xxxxxxxxxxxxx
RAZORPAY_SECRET=your_razorpay_secret

# App Configuration
APP_NAME=MLM Real Estate
COMPANY_NAME=MLM Real Estate Inc.
SUPPORT_EMAIL=support@mlmrealestate.com
SUPPORT_PHONE=+1-xxx-xxx-xxxx

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_CRASHLYTICS=true
ENABLE_BIOMETRIC_AUTH=true
ENABLE_SOCIAL_LOGIN=true

# Social Media Links
FACEBOOK_URL=https://facebook.com/mlmrealestate
INSTAGRAM_URL=https://instagram.com/mlmrealestate
TWITTER_URL=https://twitter.com/mlmrealestate
LINKEDIN_URL=https://linkedin.com/company/mlmrealestate

# App Store Links
PLAY_STORE_URL=https://play.google.com/store/apps/details?id=com.mlm.realestate.app
APP_STORE_URL=https://apps.apple.com/app/mlm-real-estate/idXXXXXXXXXX

# Terms and Privacy
TERMS_URL=https://mlmrealestate.com/terms
PRIVACY_URL=https://mlmrealestate.com/privacy
